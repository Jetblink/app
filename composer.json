{
    "name": "tree6bee/project",
    "description": "create a new ctx framework application project.",
    "type": "project",
    "keywords": ["application", "tree6bee"],
    "license": "MIT",

    "require": {
        "php": ">=5.6",
        "tree6bee/framework": "~1.0",
        "phpctx/ctx": "~1.0",
        "tree6bee/queue": "~1.0",
        "vlucas/phpdotenv": "~2.2",
        "predis/predis": "^1.1",
        "twig/twig": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/app/",
            "Ctx\\": "ctx/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Ctx\\": "ctx/tests/"
        }
    },
    "repositories": [
        { "type": "git", "url": "https://github.com/Tree6bee/support" },
        { "type": "git", "url": "https://github.com/Tree6bee/framework" },
        { "type": "git", "url": "https://github.com/Tree6bee/queue" },
        { "type": "git", "url": "https://github.com/phpctx/ctx" }
    ]
}
